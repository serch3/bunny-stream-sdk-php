{
    "name": "serch3/bunny-stream-sdk-php",
    "type": "library",
    "license": "MIT",
    "description": "A Lightweight PHP SDK for Bunny.net Streaming API",
    "keywords": ["bunnycdn", "bunny.net", "streaming", "video", "cdn"],
    "homepage": "https://bunny.net/stream/",
    "authors": [
        {
          "name": "Sergio Mandujano"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bunny\\Stream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "config": {
        "platform": {
            "php": "8.4.0"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}